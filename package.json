{
  "name": "codeshare",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start:pm2": "pm2 start ecosystem.config.json",
    "stop:pm2": "pm2 stop all",
    "logs:pm2": "pm2 logs",
    "build": "next build",
    "build:wss": "tsup src/server/wsServer.ts --format esm --env.NODE_ENV production --outDir dist/server --minify",
    "clean": "rimraf dist .next",
    "start:prod": "concurrently \"npm run next-start\" \"npm run prod:wss\"",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "dev:wss": "cross-env PORT=3001 tsx watch ./src/server/wsServer.ts --tsconfig tsconfig.json",
    "dev:next": "next dev",
    "dev": "run-p dev:*",
    "postinstall": "prisma generate",
    "prod:wss": "cross-env NODE_ENV=production WS_PORT=3001 node dist/server/wsServer.js",
    "next-start": "next start",
    "lint": "next lint"
  },
